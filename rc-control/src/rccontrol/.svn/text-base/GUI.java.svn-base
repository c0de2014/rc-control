    private void formWindowClosing(java.awt.event.WindowEvent evt) {//GEN-FIRST:event_formWindowClosing

        JOptionPane.showMessageDialog(null, "Make sure that RC-Modell is off!\n Switch back to manual control!", "Close", JOptionPane.OK_OPTION);

        // TODO add your handling code here:
        try {
            int[] x = {0, 127, 127, 127};
// stop() is needed because output must not be stopped threadsafe, safe values
// are sent by the following code
            callback.output.stop();
            callback.output.send_values(x);
            Thread.sleep(50);
            callback.output.send_values(x);
            Thread.sleep(50);
            callback.output.send_values(x);
            Thread.sleep(50);
            callback.output.send_values(x);

        } catch (Exception e) {
            System.out.println("Exception at formWindowsClosing() - GUI");

        }


    }//GEN-LAST:event_formWindowClosing

