    public void showInputValues(int[] values){
        for (int i = 0; i < callback.configuration.channelCount; i++) {
//                                        System.out.print("Value[" + i + "-"
            //+ callback.configuration.channelMapping[i]
//                            + "]: " + inValues[i]+"  ");
            System.out.print(" -> " //+ callback.configuration.channelMapping[i]
                    + values[i] + "\t");
        }
        System.out.print("\n");

                this.jSlider5.setValue(values[0]);
                this.jSlider12.setValue(values[1]);
                this.jSlider6.setValue(values[2]);
                this.jSlider11.setValue(values[3]);

    }

